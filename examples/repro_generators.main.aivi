module repro_generators.main

export main, printGen

main : Effect Text Unit
main = effect {
  _ <- print "--- Basic Yield ---"
  gen1 = generate {
    yield 1
    yield 2
    yield 3
  }
  _ <- printGen gen1
}

printGen = g => effect {
  f1 = acc => x => [x, ...acc]
  asList = gen => gen f1 []
  list = asList g
  _ <- print "{reverse list}"
}
