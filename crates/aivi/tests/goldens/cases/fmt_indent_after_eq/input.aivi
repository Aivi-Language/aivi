module test.fmt_indent_after_eq
export activeNames

class Functor (F A) = given (A: Any) {
  map: (A -> B) -> F B
}

class Apply (F A) = given (A: Any),
Functor {
  ap: F (A -> B) -> F B
}

users = [
  { name: "Alice", active: True }
  { name: "Bob", active: False }
]

activeNames = users
|> filter active
|> map.name
|> sort
|> join ", "

