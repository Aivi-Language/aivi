@no_prelude
module integrationTests.syntax.modules.useAliasAndSelectiveImports

use aivi.text as text
use aivi.text
use aivi.testing

len = length
upper = toUpper
qualifiedLen = text.length

@test "use alias and selective imports"
importsWork = do Effect {
  assertEq (len "abc") 3
  assertEq (upper "ab") "AB"
  assertEq (qualifiedLen "abcd") 4
}
