@no_prelude
module integrationTests.syntax.domains.suffixApplicationExpr

use aivi
use aivi.duration
use aivi.duration
use aivi.testing

span0 : Span
span0 = { millis: 0 }
x = 2
delta : Delta
delta = (x)min
span1 = span0 + delta

@test "suffix application"
suffixApplicationWorks = do Effect {
  assertEq span1.millis 120000
}
