@no_prelude
module integrationTests.syntax.sigils.collectionsStructured

use aivi
use aivi.collections
use aivi.testing

users = ~map{
  "id-1" => { name: "Alice" }
  "id-2" => { name: "Bob" }
}

baseTags = ~set["base"]
tags = ~set[...baseTags, "hot", "new"]

@test "structured sigils work"
structuredSigilsWork = do Effect {
  assertEq (Map.size users) 2
  assert (Set.has "hot" tags)
}
