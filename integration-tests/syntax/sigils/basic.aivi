@no_prelude
module integrationTests.syntax.sigils.basic

use aivi
use aivi.regex
use aivi.testing
use aivi.url

pattern = ~r/\w+@\w+\.\w+/
endpoint = ~u(https://api.example.com)

@test
sigilsWork = effect {
  _ <- testing.assert (regex.test pattern "a@b.com")
  _ <- assertEq endpoint.protocol "https"
}
