@no_prelude
module integrationTests.syntax.operators.listConcatOperator

use aivi
use aivi.collections (domain Collections)
use aivi.testing

out1 = [1, 2] ++ [3]

emptyInts : List Int
emptyInts = []

out2 = emptyInts ++ [1] ++ [2, 3]

@test
listConcatWorks = effect {
  _ <- assertEq out1 [1, 2, 3]
  _ <- assertEq out2 [1, 2, 3]
}
