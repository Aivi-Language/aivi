@no_prelude
module integrationTests.syntax.operators.domainOperatorResolution

use aivi
use aivi.duration
use aivi.duration
use aivi.testing

span0 : Span
span0 = { millis: 0 }

spanPlusDelta = span0 + 1s
spanMinusDelta = { millis: 1000 } - (1s)

spanPlusSpan = { millis: 1000 } + { millis: 2000 }

@test
domainOperatorsResolve = do Effect {
  assertEq spanPlusDelta.millis 1000
  assertEq spanMinusDelta.millis 0
  assertEq spanPlusSpan.millis 3000
}
