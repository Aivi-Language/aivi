@no_prelude
module integrationTests.syntax.operators.precedenceAndPipes

use aivi
use aivi.testing

map = f xs => xs match
  | []           => []
  | [x, ...rest] => [f x, ...(map f rest)]

values = [1, 2, 3]
out = values |> map (n => n * 2 + 1)
expected = [3, 5, 7]

@test "operators work"
operatorsWork = do Effect {
  assertEq out expected
}
