@no_prelude
module integrationTests.syntax.patternMatching.listsAndRecords

use aivi
use aivi.testing

head = xs => xs ?
  | []           => None
  | [x, ...rest] => Some x

getName = user => user ?
  | { name } => Some name
  | _        => None

@test
patternsWork = effect {
  assertEq (head([1, 2])) (Some 1)
  assertEq (head([])) None
  assertEq (getName { name: "A", age: 1 }) (Some "A")
}
