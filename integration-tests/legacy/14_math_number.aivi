module examples.stdlib_math_number
export main

use aivi.console (print)
use aivi.math
use aivi.number.decimal (fromFloat, domain Decimal)

main : Effect Text Unit
main = effect {
  g = gcd 54 24
  print "gcd: {g}"

  fact = factorial 10
  print "10! = {fact}"

  price = fromFloat 19.99
  total = price + fromFloat 0.01
  print "total: {total}"

  unit = sin (degrees 90.0)
  print "sin: {unit}"
}
