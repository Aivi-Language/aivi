module examples.stdlib_quaternion
export main

use aivi.console
use aivi.number.quaternion
use aivi.testing


main : Effect Text Unit
main = effect {
  axis = { x: 0.0, y: 1.0, z: 0.0 }
  spin = fromAxisAngle axis 1.570796
  unit = normalize spin
  len = magnitude unit
  print "quat len: {len}"
}

__test_sentinel = 0

@test
smoke = effect {
  _ <- pure main
  _ <- assert True
}
