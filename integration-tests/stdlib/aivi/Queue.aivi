@no_prelude
module integrationTests.stdlib.aivi.Queue

use aivi
use aivi.testing
use aivi

@test
queueOps = effect {
  q0 = Queue.empty
  q1 = Queue.enqueue q0 1
  q2 = Queue.enqueue q1 2

  assert (Queue.peek q2 == Some 1)

  res = Queue.dequeue q2
  res ?
    | Some (val, q3) => effect {
      assertEq val 1
      assert (Queue.peek q3 == Some 2)
    }
    | None => assert False
}
