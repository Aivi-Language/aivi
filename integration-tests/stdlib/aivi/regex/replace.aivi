@no_prelude
module integrationTests.stdlib.aivi.regex.replace

use aivi
use aivi.testing

use aivi.regex

subject = replace

@test
smoke = effect {
  reResult = compile "l"
  r <- reResult ?
    | Ok re => effect { pure re }
    | Err _ => effect { fail "compile failed" }
  _ <- testing.assert (replace r "hello" "L" == "heLlo")
}
