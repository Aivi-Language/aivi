@no_prelude
module integrationTests.stdlib.aivi.regex.split

use aivi
use aivi.testing

use aivi.regex

subject = split

@test
smoke = effect {
  reResult = compile ","
  r <- reResult ?
    | Ok re => effect { pure re }
    | Err _ => effect { fail "compile failed" }

  _ <- testing.assert (split r "a,b,c" == ["a", "b", "c"])
}
