@no_prelude
module integrationTests.stdlib.aivi.regex.test

use aivi
use aivi.testing (assert)

use aivi.regex

subject = test

@test
smoke = effect {
  reResult = compile "abc"
  r <- reResult ?
    | Ok re => effect.pure re
    | Err _ => effect.fail "compile failed"

  _ <- assert (test r "abc" == True)
  _ <- assert (test r "123" == False)
}
