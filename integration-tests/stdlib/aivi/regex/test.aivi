@no_prelude
module integrationTests.stdlib.aivi.regex.test

use aivi
use aivi.testing (assert)

use aivi.regex

subject = test

@test
smoke = effect {
  r <- match compile "abc" {
    | Ok re => pure re
    | Err _ => fail "compile failed"
  }
  _ <- assert (test r "abc" == True)
  _ <- assert (test r "123" == False)
}
