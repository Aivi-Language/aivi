@no_prelude
module integrationTests.stdlib.aivi.regex.test

use aivi
use aivi.testing

use aivi.regex

subject = test

@test
smoke = effect {
  reResult = compile "abc"
  r <- reResult ?
    | Ok re => effect { pure re }
    | Err _ => effect { fail "compile failed" }

  _ <- testing.assert (test r "abc" == True)
  _ <- testing.assert (test r "123" == False)
}
