@no_prelude
module integrationTests.stdlib.aivi.regex.matches

use aivi
use aivi.testing

use aivi.regex



@test
smoke = effect {
  reResult = compile "l"
  r <- reResult ?
    | Ok re => effect { pure re }
    | Err _ => effect { fail "compile failed" }
  ms = matches r "hello"
  _ <- assert (length ms == 2)
}
