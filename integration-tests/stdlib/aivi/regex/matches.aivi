@no_prelude
module integrationTests.stdlib.aivi.regex.matches

use aivi
use aivi.testing (assert)

use aivi.regex

subject = matches

@test
smoke = effect {
  r <- match compile "l" {
    | Ok re => pure re
    | Err _ => fail "compile failed"
  }
  ms = matches r "hello"
  _ <- assert (length ms == 2)
}
