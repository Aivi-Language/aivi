@no_prelude
module integrationTests.stdlib.aivi.regex.replaceAll

use aivi
use aivi.testing (assert)

use aivi.regex

subject = replaceAll

@test
smoke = effect {
  r <- match compile "l" {
    | Ok re => pure re
    | Err _ => fail "compile failed"
  }
  _ <- assert (replaceAll r "hello" "L" == "heLLo")
}
