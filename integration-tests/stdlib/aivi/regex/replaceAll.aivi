@no_prelude
module integrationTests.stdlib.aivi.regex.replaceAll

use aivi
use aivi.testing

use aivi.regex

subject = replaceAll

@test
smoke = effect {
  reResult = compile "l"
  r <- reResult ?
    | Ok re => effect { pure re }
    | Err _ => effect { fail "compile failed" }
  _ <- testing.assert (replaceAll r "hello" "L" == "heLLo")
}
