@no_prelude
module integrationTests.stdlib.aivi.calendar.addYears

use aivi
use aivi.testing

use aivi.calendar

subject = addYears

@test
smoke = effect {
  d = addYears 1 { year: 2024, month: 2, day: 29 }
  _ <- testing.assert (d.year == 2025)
  _ <- testing.assert (d.day == 28) // 2025 is not leap, clamps to 28
}
