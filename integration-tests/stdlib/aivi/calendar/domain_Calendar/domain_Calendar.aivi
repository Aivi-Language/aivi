@no_prelude
module integrationTests.stdlib.aivi.calendar.domain_Calendar.tests

use aivi
use aivi.testing
use aivi.calendar

@test
literal_eom_smoke = effect {
  assert (eom == { year: 2024, month: 2, day: 29 }) // Wait, eom is usually used with a date.
}

@test
suffix_d_smoke = effect {
  d = 5d
  assert (d == 5) // Duration in days?
}

@test
suffix_m_smoke = effect {
  m = 1m
  assert (m == 1)
}

@test
suffix_y_smoke = effect {
  y = 1y
  assert (y == 1)
}
