@no_prelude
module integrationTests.stdlib.aivi.calendar.daysInMonth

use aivi
use aivi.testing (assert)

use aivi.calendar

subject = daysInMonth

@test
smoke = effect {
  _ <- assert (daysInMonth { year: 2024, month: 2, day: 1 } == 29)
  _ <- assert (daysInMonth { year: 2023, month: 2, day: 1 } == 28)
}
