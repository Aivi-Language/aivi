@no_prelude
module integrationTests.stdlib.aivi.calendar.isLeapYear

use aivi
use aivi.testing (assert)

use aivi.calendar

subject = isLeapYear

@test
smoke = effect {
  _ <- assert (isLeapYear { year: 2024, month: 1, day: 1 } == True)
  _ <- assert (isLeapYear { year: 2023, month: 1, day: 1 } == False)
}
