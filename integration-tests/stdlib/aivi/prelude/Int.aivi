@no_prelude
module integrationTests.stdlib.aivi.prelude.Int

use aivi
use aivi.testing
use aivi.prelude

@test
intOps = effect {
  _ <- assertEq (1 + 2) 3
  _ <- assertEq (2 * 3) 6
  _ <- assertEq (4 / 2) 2
  _ <- assertEq (5 - 3) 2
  _ <- assert (1 < 2)
  _ <- assert (2 > 1)
  _ <- assert (1 <= 1)
  _ <- assert (1 >= 1)
  _ <- assertEq (Int.min 1 2) 1
  _ <- assertEq (Int.max 1 2) 2
}
