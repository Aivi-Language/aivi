@no_prelude
module integrationTests.stdlib.aivi.prelude.Option

use aivi
use aivi.testing
use aivi.prelude

@test
optionMatches = effect {
  some = Some 1
  _ <- some ?
    | Some n => assertEq n 1
    | None   => assert False

    none = None
    _ <- none ?
      | Some _ => assert False
      | None   => assert True
}
