@no_prelude
module integrationTests.stdlib.aivi.prelude.Result

use aivi
use aivi.testing
use aivi.prelude

@test
resultMatches = effect {
  ok = Ok 1
  _ <- ok ?
    | Ok n  => assertEq n 1
    | Err _ => assert False

  err = Err "nope"
  _ <- err ?
    | Ok _  => assert False
    | Err e => assertEq e "nope"
}
