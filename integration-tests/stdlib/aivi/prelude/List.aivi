@no_prelude
module integrationTests.stdlib.aivi.prelude.List

use aivi
use aivi.testing
use aivi.prelude

len = xs => xs ?
  | []           => 0
  | [_, ...rest] => 1 + len rest

map = f xs => xs ?
  | []           => []
  | [x, ...rest] => [f x, ...(map f rest)]

@test
listFns = effect {
  l = [1, 2, 3]
  _ <- assertEq (len l) 3
  _ <- assertEq (map (x => x + 1) l)[2, 3, 4]
}
