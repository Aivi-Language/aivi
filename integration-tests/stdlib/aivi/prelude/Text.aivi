@no_prelude
module integrationTests.stdlib.aivi.prelude.Text

use aivi
use aivi.testing
use aivi.prelude

@test
textFns = effect {
  t = "Hello"
  _ <- assertEq (Text.length t) 5
  _ <- assertEq (Text.toUpper t) "HELLO"
  _ <- assertEq (Text.toLower t) "hello"
  _ <- assertEq (Text.slice 0 2 t) "He"
  _ <- assert (Text.isEmpty "" == True)
  _ <- assert (Text.isEmpty t == False)
}
