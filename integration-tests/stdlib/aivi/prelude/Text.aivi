@no_prelude
module integrationTests.stdlib.aivi.prelude.Text

use aivi
use aivi.testing
use aivi.prelude
use aivi.text

@test
textFns = effect {
  t = "Hello"
  _ <- assertEq (length t) 5
  _ <- assertEq (toUpper t) "HELLO"
  _ <- assertEq (toLower t) "hello"
  _ <- assert (isEmpty "" == True)
  _ <- assert (isEmpty t == False)
}
