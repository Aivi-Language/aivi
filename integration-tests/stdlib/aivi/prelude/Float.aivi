@no_prelude
module integrationTests.stdlib.aivi.prelude.Float

use aivi
use aivi.testing
use aivi.prelude

@test
floatOps = effect {
  _ <- assert (1.0 + 2.0 == 3.0)
  _ <- assert (2.0 * 3.0 == 6.0)
  _ <- assert (4.0 / 2.0 == 2.0)
  _ <- assert (1.0 < 2.0)
  _ <- assert (2.0 > 1.0)
  _ <- assert (1.0 <= 1.0)
  _ <- assert (1.0 >= 1.0)
}
