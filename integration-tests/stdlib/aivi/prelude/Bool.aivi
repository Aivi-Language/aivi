@no_prelude
module integrationTests.stdlib.aivi.prelude.Bool

use aivi
use aivi.testing
use aivi.prelude

@test
boolOps = effect {
  _ <- assert (True && True)
  _ <- assert (True || False)
  _ <- assert (False || True)
  _ <- assert (not False)
  _ <- assert (not (True && False))
  _ <- assert (True != False)
  _ <- assert (True == True)
}
