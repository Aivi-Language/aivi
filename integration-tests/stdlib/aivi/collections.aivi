@no_prelude
module integrationTests.stdlib.aivi.collections

use aivi
use aivi.testing
use aivi

@test
smoke = effect {
  base = ~map{ "a" => 1, "b" => 2 }
  more = Map.insert "c" 3 base
  _ <- assert (Map.get more "a" == Some 1)
  _ <- assert (Map.get more "c" == Some 3)
  _ <- assert (Map.get more "missing" == None)
  set = ~set["x", "y"]
  _ <- assert (Set.has "x" set)
}
