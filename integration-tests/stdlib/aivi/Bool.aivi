@no_prelude
module integrationTests.stdlib.aivi.Bool

use aivi
use aivi.testing

use aivi

Ref = Bool

@test
smoke = effect {
  _ <- testing.assert (True && True)
  _ <- testing.assert (True || False)
  _ <- testing.assert (False || True)
  _ <- testing.assert (not False)
  _ <- testing.assert (not (True && False))
  _ <- testing.assert (True != False)
  _ <- testing.assert (True == True)
}
