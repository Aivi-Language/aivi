@no_prelude
module integrationTests.stdlib.aivi.Bool

use aivi
use aivi.testing (assert)

use aivi

Ref = Bool

@test
smoke = effect {
  _ <- assert (True && True)
  _ <- assert (True || False)
  _ <- assert (False || True)
  _ <- assert (not False)
  _ <- assert (not (True && False))
  _ <- assert (True != False)
  _ <- assert (True == True)
}
