@no_prelude
module integrationTests.stdlib.aivi.Text

use aivi
use aivi.testing

use aivi

@test
smoke = effect {
  t = "Hello"
  _ <- assert (Text.length t == 5)
  _ <- assert (Text.toUpper t == "HELLO")
  _ <- assert (Text.toLower t == "hello")
  _ <- assert (Text.slice 0 2 t == "He")
  _ <- assert (Text.isEmpty "" == True)
  _ <- assert (Text.isEmpty t == False)
}
