@no_prelude
@no_prelude
module integrationTests.stdlib.aivi.text

use aivi
use aivi.testing
use aivi.text

@test
smoke = effect {
  txt = "Hello, Test"
  assert (length txt == 11)
  assert (toLower txt == "hello, test")
  assert (contains txt "Test")
  assert (startsWith txt "Hello")
  assert (titleCase txt == "Hello, Test")
}
