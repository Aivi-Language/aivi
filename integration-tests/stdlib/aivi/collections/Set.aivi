@no_prelude
module integrationTests.stdlib.aivi.collections.Set

use aivi
use aivi.testing

use aivi.collections



@test
smoke = effect {
  s0 = Set.empty
  s1 = Set.insert 1 s0
  _ <- testing.assert (Set.size s0 == 0)
  _ <- testing.assert (Set.size s1 == 1)
  _ <- testing.assert (Set.has 1 s1 == True)
}
