@no_prelude
module integrationTests.stdlib.aivi.List

use aivi
use aivi.testing
use aivi

@test
smoke = effect {
  l = [1, 2, 3]
  assert (List.length l == 3)
  assert (List.at l 0 == Some 1)
  assert (List.at l 3 == None)

  l2 = List.map l (x => x + 1)
  assert (l2 == [2, 3, 4])

  l3 = List.filter l (x => x > 1)
  assert (l3 == [2, 3])

  assert (List.isEmpty[] == True)
  assert (List.isEmpty l == False)
}
