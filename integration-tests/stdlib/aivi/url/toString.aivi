@no_prelude
module integrationTests.stdlib.aivi.url.toString

use aivi
use aivi.testing (assert)

use aivi.url

subject = toString

@test
smoke = effect {
  u = { protocol: "http", host: "example.com", port: None, path: "/", query: [], hash: None }
  s = toString u
  _ <- assert (s == "http://example.com/")
}
