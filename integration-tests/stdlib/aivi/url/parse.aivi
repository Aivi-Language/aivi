@no_prelude
module integrationTests.stdlib.aivi.url.parse

use aivi
use aivi.testing

use aivi.url



@test
smoke = effect {
  res = parse "https://example.com:8080/path?q=v#hash"
  r <- res ?
    | Ok re => effect { pure re }
    | Err _ => effect { fail "parse failed" }
  _ <- testing.assert (r.protocol == "https")
  _ <- testing.assert (r.host == "example.com")
  _ <- testing.assert (r.port == Some 8080)
}
