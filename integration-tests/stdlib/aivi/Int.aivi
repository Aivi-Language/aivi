@no_prelude
module integrationTests.stdlib.aivi.Int

use aivi
use aivi.testing

use aivi



@test
smoke = effect {
  _ <- testing.assert (1 + 2 == 3)
  _ <- testing.assert (2 * 3 == 6)
  _ <- testing.assert (4 / 2 == 2)
  _ <- testing.assert (5-3 == 2)
  _ <- testing.assert (1 < 2)
  _ <- testing.assert (2 > 1)
  _ <- testing.assert (1 <= 1)
  _ <- testing.assert (1 >= 1)
}
