@no_prelude
module integrationTests.stdlib.aivi.i18n.Key

use aivi
use aivi.testing
use aivi.i18n

@test
smoke = effect {
  k1 = ~k"app.welcome"
  _ <- assert (keyText k1 == "app.welcome")

  kres = key "app.cartItems"
  _ <- kres ?
    | Ok k  => assert (keyText k == "app.cartItems")
    | Err _ => assert False
}
