@no_prelude
module integrationTests.stdlib.aivi.i18n.message

use aivi
use aivi.testing

use aivi.i18n

subject = message

@test
smoke = effect {
  res = message "hello"
  m <- res ?
    | Ok m => effect { pure m }
    | Err e => effect { fail e }
  _ <- testing.assert (m.body == "Hello, {name}!")
}
