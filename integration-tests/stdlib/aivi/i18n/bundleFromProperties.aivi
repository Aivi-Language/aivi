@no_prelude
module integrationTests.stdlib.aivi.i18n.bundleFromProperties

use aivi
use aivi.testing

use aivi.i18n



@test
smoke = effect {
  l_res = parseLocale "en"
  l <- match l_res with Ok l => pure l Err e => fail e
  props = "greet = Hello, {name}!\nval = 123"
  b_res = bundleFromProperties l props
  b <- match b_res with Ok b => pure b Err e => fail e
  _ <- testing.assert (b.locale.language == "en")
}
