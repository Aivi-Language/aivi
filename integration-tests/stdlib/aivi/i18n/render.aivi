@no_prelude
module integrationTests.stdlib.aivi.i18n.render

use aivi
use aivi.testing
use aivi.i18n

@test
smoke = effect {
  m_res = message "Hello!"
  m <- m_res ?
    | Ok m  => pure m
    | Err e => fail e

    r_res = render m {}
    r <- r_res ?
      | Ok r  => pure r
      | Err e => fail e

    _ <- assert (r == "Hello!")
}
