@no_prelude
module integrationTests.stdlib.aivi.i18n.render

use aivi
use aivi.testing

use aivi.i18n



@test
smoke = effect {
  m_res = message "Hello, {name}!"
  m <- m_res ?
    | Ok m => effect { pure m }
    | Err e => effect { fail e }
  r_res = render m { name: "Antigravity" }
  r <- r_res ?
    | Ok r => effect { pure r }
    | Err e => effect { fail e }
  _ <- testing.assert (r == "Hello, Antigravity!")
}
