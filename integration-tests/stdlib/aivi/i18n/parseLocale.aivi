@no_prelude
module integrationTests.stdlib.aivi.i18n.parseLocale

use aivi
use aivi.testing

use aivi.i18n



@test
smoke = effect {
  res = parseLocale "en-US"
  r <- res ?
    | Ok x => effect { pure x }
    | Err _ => effect { fail "parse failed" }
  _ <- testing.assert (r.language == "en")
  _ <- testing.assert (r.region == Some "US")
}
