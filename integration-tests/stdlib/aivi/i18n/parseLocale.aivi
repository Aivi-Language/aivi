@no_prelude
module integrationTests.stdlib.aivi.i18n.parseLocale

use aivi
use aivi.testing (assert)

use aivi.i18n

subject = parseLocale

@test
smoke = effect {
  res = parseLocale "en-US"
  r <- match res with
    Ok l => pure l
    Err e => fail e
  _ <- assert (r.language == "en")
  _ <- assert (r.region == Some "US")
}
