@no_prelude
module integrationTests.stdlib.aivi.ui.layout.tests

use aivi
use aivi.testing
use aivi.ui.layout

length_touch : Length -> Unit
length_touch = _ => Unit

@test
length_smoke = effect {
  _ <- pure length_touch
  _ <- assertEq (1 + 1) 2
}

percentage_touch : Percentage -> Unit
percentage_touch = _ => Unit

@test
percentage_smoke = effect {
  _ <- pure percentage_touch
  _ <- assertEq (1 + 1) 2
}

unitVal_touch : UnitVal -> Unit
unitVal_touch = _ => Unit

@test
unitVal_smoke = effect {
  _ <- pure unitVal_touch
  _ <- assertEq (1 + 1) 2
}
