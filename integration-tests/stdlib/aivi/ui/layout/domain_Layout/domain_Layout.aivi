@no_prelude
module integrationTests.stdlib.aivi.ui.layout.domain_Layout.tests

use aivi
use aivi.testing
use aivi.ui.layout
use aivi.ui.layout (domain Layout)

@test "literals"
literals_functional = do Effect {
  x <- 1px
  y <- 1em
  z <- 1rem
  a <- 1vh
  b <- 1vw
  assertEq x (Px 1)
  assertEq y (Em 1)
  assertEq z (Rem 1)
  assertEq a (Vh 1)
  assertEq b (Vw 1)
}
