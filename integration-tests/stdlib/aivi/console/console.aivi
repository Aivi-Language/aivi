@no_prelude
module integrationTests.stdlib.aivi.console.tests

use aivi
use aivi.testing
use aivi.console

@test
error_smoke = do Effect {
  _ <- error "err"
  assert True
}

@test
log_smoke = do Effect {
  _ <- log "log"
  assert True
}

@test
print_smoke = do Effect {
  _ <- print "p"
  assert True
}

@test
println_smoke = do Effect {
  _ <- println "pl"
  assert True
}

@test
readLine_smoke = do Effect {
  assert True
  assert True
}

@test
strip_smoke = do Effect {
  assertEq (strip "plain") "plain"
}
