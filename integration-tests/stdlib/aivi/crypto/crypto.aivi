@no_prelude
module integrationTests.stdlib.aivi.crypto.tests

use aivi
use aivi.testing
use aivi.crypto
use aivi.text

@test
randomBytes_smoke = effect {
  b <- randomBytes 16
  pure b
  assertEq (1 + 1) 2
}

@test
randomUuid_smoke = effect {
  u <- randomUuid
  assert (length u == 36)
}

@test
sha256_smoke = effect {
  s = sha256 "hello"
  assert (s == "2cf24dba5fb0a30e26e83b2ac5b9e29e1b161e5c1fa7425e73043362938b9824")
}
