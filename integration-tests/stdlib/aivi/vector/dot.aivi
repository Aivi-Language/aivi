@no_prelude
module integrationTests.stdlib.aivi.vector.dot

use aivi
use aivi.testing

use aivi.vector



@test
smoke = effect {
  v1 = { x: 1.0, y: 0.0 }
  v2 = { x: 0.0, y: 1.0 }
  _ <- testing.assert (abs (dot v1 v2) < 0.0001)

  v3 = { x: 1.0, y: 1.0 }
  _ <- testing.assert (abs (dot v1 v3 - 1.0) < 0.0001)
}
