@no_prelude
module integrationTests.stdlib.aivi.vector.Vec3

use aivi
use aivi.testing
use aivi.vector
use aivi.math

@test
smoke = effect {
  v = { x: 1.0, y: 2.0, z: 3.0 }
  _ <- assert (v.z == 3.0)
  _ <- assert (cross { x: 1.0, y: 0.0, z: 0.0 } { x: 0.0, y: 1.0, z: 0.0 } == { x: 0.0, y: 0.0, z: 1.0 })
}
