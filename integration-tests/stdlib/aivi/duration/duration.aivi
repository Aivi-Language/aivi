@no_prelude
module integrationTests.stdlib.aivi.duration.tests

use aivi
use aivi.testing
use aivi.duration

span_touch : Span -> Unit
span_touch = _ => Unit

@test
span_smoke = effect {
  _ <- pure span_touch
  _ <- assertEq (1 + 1) 2
}

@test
negateDelta_smoke = effect {
  _ <- assert (negateDelta 1s == -1s)
}
