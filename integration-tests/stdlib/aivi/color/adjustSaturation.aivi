@no_prelude
module integrationTests.stdlib.aivi.color.adjustSaturation

use aivi
use aivi.testing
use aivi.color

@test
smoke = effect {
  rgb = { r: 100, g: 100, b: 100 }
  more = adjustSaturation rgb 20
  less = adjustSaturation rgb (-20)
  _ <- assert (more.r >= rgb.r)
  _ <- assert (less.r <= rgb.r)
}
