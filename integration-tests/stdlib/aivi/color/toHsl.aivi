@no_prelude
module integrationTests.stdlib.aivi.color.toHsl

use aivi
use aivi.testing
use aivi.color

@test
smoke = effect {
  rgb = { r: 100, g: 150, b: 200 }
  hsl = toHsl rgb
  recon = toRgb hsl
  _ <- assert (abs (recon.r - rgb.r) < 10)
  _ <- assert (abs (recon.g - rgb.g) < 10)
  _ <- assert (abs (recon.b - rgb.b) < 10)
}
