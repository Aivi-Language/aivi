@no_prelude
module integrationTests.stdlib.aivi.color.adjustLightness

use aivi
use aivi.testing
use aivi.color

@test
smoke = effect {
  rgb = { r: 20, g: 40, b: 60 }
  lighter = adjustLightness rgb 10
  darker = adjustLightness rgb (-10)
  _ <- assert (lighter.r >= rgb.r)
  _ <- assert (darker.r <= rgb.r)
}
