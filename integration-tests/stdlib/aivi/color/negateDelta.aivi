@no_prelude
module integrationTests.stdlib.aivi.color.negateDelta

use aivi
use aivi.testing
use aivi.color

@test
smoke = effect {
  light = negateDelta (Lightness 5)
  sat = negateDelta (Saturation 10)
  hue = negateDelta (Hue 15)
  _ <- assert (light == Lightness (-5))
  _ <- assert (sat == Saturation (-10))
  _ <- assert (hue == Hue (-15))
}
