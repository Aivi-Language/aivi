@no_prelude
module integrationTests.stdlib.aivi.text.tests

use aivi
use aivi.testing
use aivi.text

bytes_touch : Bytes -> Unit
bytes_touch = _ => Unit

@test
bytes_smoke = effect {
  _ <- pure bytes_touch
  _ <- assertEq (1 + 1) 2
}

