@no_prelude
module integrationTests.stdlib.aivi.text.parseFloat

use aivi
use aivi.testing
use aivi.text

@test
smoke = effect {
  _ <- assert (parseFloat "123.45" == Some 123.45)
  // _ <- assert (parseFloat "abc" == None)? Not sure if it yields None or error. Assuming Option return.
  res = parseFloat "abc"
  _ <- res ?
    | Some _ => assert False
    | None   => assert True
}
