@no_prelude
module integrationTests.stdlib.aivi.signal.tests

use aivi
use aivi.testing
use aivi.signal

spectrum_touch : Spectrum -> Unit
spectrum_touch = _ => Unit

@test
spectrum_smoke = effect {
  _ <- pure spectrum_touch
  _ <- assertEq (1 + 1) 2
}

@test
fft_smoke = effect {
  _ <- pure fft
  _ <- assert True
}

@test
ifft_smoke = effect {
  _ <- pure ifft
  _ <- assert True
}

@test
normalize_smoke = effect {
  _ <- pure normalize
  _ <- assert True
}

@test
windowHann_smoke = effect {
  _ <- pure windowHann
  _ <- assert True
}
