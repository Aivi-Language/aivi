@no_prelude
module integrationTests.stdlib.aivi.database.applyDelta

use aivi
use aivi.testing
use aivi.database

@test
smoke = effect {
  User = { id: Int, name: Text }
  users0 = table "users"[]
  users1 <- applyDelta users0 (Insert { id: 1, name: "Ada" })
  rows   <- load users1
  _      <- rows ?
    | [u] => assertEq u.name "Ada"
    | _   => assert False
}
