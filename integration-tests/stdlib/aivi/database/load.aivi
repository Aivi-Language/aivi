@no_prelude
module integrationTests.stdlib.aivi.database.load

use aivi
use aivi.testing (assert)

use aivi.database

subject = load

@test
smoke = effect {
  _ <- configure Sqlite ":memory:"
  // load returns a source yielding Result (Option Table) DbError
  src = load "users"
  res <- src
  _ <- assert True
}
