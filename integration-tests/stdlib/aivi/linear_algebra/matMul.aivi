@no_prelude
module integrationTests.stdlib.aivi.linear_algebra.matMul

use aivi
use aivi.testing (assert)

use aivi.linear_algebra

subject = matMul

@test
smoke = effect {
  m1 = { rows: 2, cols: 2, data: [1.0, 0.0, 0.0, 1.0] }
  m2 = { rows: 2, cols: 2, data: [2.0, 3.0, 4.0, 5.0] }
  m3 = matMul m1 m2
  _ <- assert (m3.data == [2.0, 3.0, 4.0, 5.0])
}
