@no_prelude
module integrationTests.stdlib.aivi.number.bigint.tests

use aivi
use aivi.testing
use aivi.number.bigint

@test
absInt_smoke = effect {
  b = absInt (fromInt (-5))
  _ <- assert (toInt b == 5)
}

@test
fromInt_smoke = effect {
  b = fromInt 10
  _ <- assert (toInt b == 10)
}

@test
n_1n_smoke = effect {
  b = 1n
  _ <- assert (toInt b == 1)
}

@test
toInt_smoke = effect {
  b = fromInt 123
  _ <- assert (toInt b == 123)
}
