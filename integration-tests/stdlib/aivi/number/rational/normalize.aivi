@no_prelude
module integrationTests.stdlib.aivi.number.rational.normalize

use aivi
use aivi.testing (assert)

use aivi.number.rational

subject = normalize

@test
smoke = effect {
  r = fromInts (fromInt 2) (fromInt 4)
  n = normalize r
  _ <- assert (toInt (numerator n) == 1)
  _ <- assert (toInt (denominator n) == 2)
}
