@no_prelude
module integrationTests.stdlib.aivi.linalg.matMul

use aivi
use aivi.testing

use aivi.linalg



@test
smoke = effect {
  m1 = { rows: 1, cols: 2, data: [1.0, 2.0] }
  m2 = { rows: 2, cols: 1, data: [3.0, 4.0] }
  m3 = matMul m1 m2
  _ <- testing.assert (m3.rows == 1)
  _ <- testing.assert (m3.cols == 1)
  _ <- testing.assert (m3.data == [11.0])
}
