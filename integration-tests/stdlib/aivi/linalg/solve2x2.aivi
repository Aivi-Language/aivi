@no_prelude
module integrationTests.stdlib.aivi.linalg.solve2x2

use aivi
use aivi.testing

use aivi.linalg



@test
smoke = effect {
  // x + y = 3
  // x - y = 1
  // => x=2, y=1
  m = { rows: 2, cols: 2, data: [1.0, 1.0, 1.0, -1.0] }
  v = { size: 2, data: [3.0, 1.0] }
  res = solve2x2 m v
  _ <- assert (res.data == [2.0, 1.0])
}
