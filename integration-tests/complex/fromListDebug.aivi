@no_prelude
module integrationTests.complex.fromListDebug

use aivi
use aivi.testing
use aivi.generator

@test "fromList basic roundtrip"
fromListBasic = do Effect {
  result = toList (fromList [1, 2, 3])
  assertEq result [1, 2, 3]
}

@test "fromList empty"
fromListEmpty = do Effect {
  result = toList (fromList [])
  assertEq result []
}

@test "foldl on fromList"
foldlFromList = do Effect {
  total = foldl (acc x => acc + x) 0 (fromList [1, 2, 3, 4, 5])
  assertEq total 15
}
