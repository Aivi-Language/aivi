{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "AIVI Themes",
  "author": "AIVI",
  "themes": [
    {
      "name": "AIVI Dark Modern",
      "appearance": "dark",
      "style": {
        "background":                        "#1F1F1F",
        "foreground":                        "#CCCCCC",
        "border":                            "#2B2B2B",
        "border.variant":                    "#3C3C3C",
        "border.focused":                    "#0078D4",
        "border.selected":                   "#0078D4",
        "border.transparent":                "#00000000",
        "border.disabled":                   "#3C3C3C",

        "elevated_surface.background":       "#202020",
        "surface.background":                "#181818",
        "panel.background":                  "#181818",
        "pane.focused_border":               "#0078D4",

        "tab_bar.background":                "#181818",
        "tab.active_background":             "#1F1F1F",
        "tab.inactive_background":           "#181818",

        "toolbar.background":                "#181818",
        "title_bar.background":              "#181818",
        "title_bar.inactive_background":     "#1F1F1F",

        "status_bar.background":             "#181818",

        "editor.background":                 "#1F1F1F",
        "editor.foreground":                 "#CCCCCC",
        "editor.gutter.background":          "#1F1F1F",
        "editor.subheader.background":       "#181818",

        "editor.active_line.background":     "#2A2D2E",
        "editor.highlighted_line.background": "#2A2D2E20",

        "editor.line_number":                "#6E7681",
        "editor.active_line_number":         "#CCCCCC",
        "editor.invisible":                  "#404040",
        "editor.wrap_guide":                 "#404040",
        "editor.active_wrap_guide":          "#707070",

        "editor.indent_guide":               "#404040",
        "editor.indent_guide_active":        "#707070",

        "editor.selection.background":       "#264F78",
        "editor.document_highlight.read_background":  "#264F7840",
        "editor.document_highlight.write_background": "#264F7860",

        "search.match_background":           "#9E6A0366",

        "terminal.background":               "#1F1F1F",
        "terminal.foreground":               "#CCCCCC",
        "terminal.bright_foreground":        "#FFFFFF",
        "terminal.dim_foreground":           "#868686",
        "terminal.ansi.black":               "#1E1E1E",
        "terminal.ansi.red":                 "#F85149",
        "terminal.ansi.green":               "#2EA043",
        "terminal.ansi.yellow":              "#D29922",
        "terminal.ansi.blue":                "#58A6FF",
        "terminal.ansi.magenta":             "#BC8CFF",
        "terminal.ansi.cyan":                "#4DC4FF",
        "terminal.ansi.white":               "#C9D1D9",
        "terminal.ansi.bright_black":        "#484F58",
        "terminal.ansi.bright_red":          "#FF7B72",
        "terminal.ansi.bright_green":        "#56D364",
        "terminal.ansi.bright_yellow":       "#E3B341",
        "terminal.ansi.bright_blue":         "#79C0FF",
        "terminal.ansi.bright_magenta":      "#D2A8FF",
        "terminal.ansi.bright_cyan":         "#56D4DD",
        "terminal.ansi.bright_white":        "#F0F6FC",

        "link_text.hover":                   "#4daafc",

        "syntax": {
          "comment":              { "color": "#6A9955", "font_style": "italic" },

          "string":               { "color": "#CE9178" },
          "string.escape":        { "color": "#CE9178" },
          "string.special":       { "color": "#565656" },
          "character":            { "color": "#CE9178" },

          "number":               { "color": "#B5CEA8" },
          "number.unit":          { "color": "#D7BA7D" },
          "constant.color":       { "color": "#4FC1FF" },
          "constant.builtin":     { "color": "#569CD6" },

          "boolean":              { "color": "#569CD6" },
          "constructor":          { "color": "#9CDCFE" },

          "keyword":              { "color": "#C586C0" },
          "keyword.control.return": { "color": "#C586C0" },
          "keyword.control.repeat": { "color": "#C586C0" },

          "operator":             { "color": "#D4D4D4" },
          "operator.special":     { "color": "#569CD6" },

          "type":                 { "color": "#4EC9B0" },
          "type.definition":      { "color": "#4EC9B0", "font_weight": 700 },
          "type.parameter":       { "color": "#DCDCAA" },

          "function":             { "color": "#DCDCAA" },
          "function.definition":  { "color": "#DCDCAA", "font_weight": 700 },

          "variable":             { "color": "#9CDCFE" },
          "variable.special":     { "color": "#D7BA7D" },

          "property":             { "color": "#9CDCFE" },
          "parameter":            { "color": "#9CDCFE" },

          "attribute":            { "color": "#92d49d" },

          "namespace":            { "color": "#CCCCCC" },

          "punctuation":          { "color": "#BFBFBF" },
          "punctuation.bracket":  { "color": "#BFBFBF" },
          "punctuation.delimiter": { "color": "#B0B0B0" },

          "tag":                  { "color": "#569CD6" },
          "tag.attribute":        { "color": "#9CDCFE" }
        }
      }
    }
  ]
}
